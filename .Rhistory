generatePlot  <- function(data, mainText, color="black") {
plot <- barplot(
height = data[1],
names = data[2],
main = mainText,
ylim = c(0, 1.15*max(data[2]))
)
text(
x = plot,
y = data[1],
label = data[1],
pos = 3,
cex = 0.8,
col = color
)
}
